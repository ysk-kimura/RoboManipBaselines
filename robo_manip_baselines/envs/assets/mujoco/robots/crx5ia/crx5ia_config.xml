<mujoco name="crx5ia_config">

  <default>
    <default class="crx5ia">
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2" density="0"/>
      </default>
      <default class="collision">
        <mesh scale="0.001 0.001 0.001"/>
        <geom type="mesh" rgba="0 0 0 0"/>
      </default>
      <default class="large_actuator">
        <position kp="2000" kv="400" forcerange="-160 160"/>
      </default>
      <default class="small_actuator">
        <position kp="500" kv="100" forcerange="-40 40"/>
      </default>
    </default>

    <!-- gripper: robotiq_2f85 -->
    <default class="2f85">
      <mesh scale="0.001 0.001 0.001"/>
      <general biastype="affine"/>

      <joint axis="1 0 0"/>
      <default class="driver">
        <joint range="0 0.8" armature="0.005" damping="0.1" solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
      </default>
      <default class="follower">
        <joint range="-0.872664 0.872664" armature="0.001" pos="0 -0.018 0.0065" solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
      </default>
      <default class="spring_link">
        <joint range="-0.29670597283 0.8" armature="0.001" stiffness="0.05" springref="2.62" damping="0.00125"/>
      </default>
      <default class="coupler">
        <joint range="-1.57 0" armature="0.001" solimplimit="0.95 0.99 0.001" solreflimit="0.005 1"/>
      </default>

      <default class="gripper_visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>
      <default class="gripper_collision">
        <geom type="mesh" group="3"/>
        <default class="pad_box1">
          <geom mass="0" type="box" pos="0 -0.0026 0.028125" size="0.011 0.004 0.009375" friction="0.7"
                solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.55 0.55 0.55 1"/>
        </default>
        <default class="pad_box2">
          <geom mass="0" type="box" pos="0 -0.0026 0.009375" size="0.011 0.004 0.009375" friction="0.6"
                solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.45 0.45 0.45 1"/>
        </default>
      </default>
    </default>
  </default>


  <asset>
    <material name="black" rgba="0.1 0.1 0.1 1"/>
    <material name="white" rgba="0.99 0.99 0.99 1"/>

    <mesh name="base" file="crx5ia/visual/base.stl"/>
    <mesh class="collision" name="base_collision" file="crx5ia/collision/base.stl"/>
    <mesh name="j1" file="crx5ia/visual/j1.stl"/>
    <mesh class="collision" name="j1_collision" file="crx5ia/collision/j1.stl"/>
    <mesh name="j2" file="crx5ia/visual/j2.stl"/>
    <mesh class="collision" name="j2_collision" file="crx5ia/collision/j2.stl"/>
    <mesh name="j3" file="crx5ia/visual/j3.stl"/>
    <mesh class="collision" name="j3_collision" file="crx5ia/collision/j3.stl"/>
    <mesh name="j4" file="crx5ia/visual/j4.stl"/>
    <mesh class="collision" name="j4_collision" file="crx5ia/collision/j4.stl"/>
    <mesh name="j5" file="crx5ia/visual/j5.stl"/>
    <mesh class="collision" name="j5_collision" file="crx5ia/collision/j5.stl"/>
    <mesh name="j6" file="crx5ia/visual/j6.stl"/>
    <mesh class="collision" name="j6_collision" file="crx5ia/collision/j6.stl"/>

    <!-- gripper: robotiq_2f85 -->
    <material name="metal" rgba="0.58 0.58 0.58 1"/>
    <material name="silicone" rgba="0.1882 0.1882 0.1882 1"/>
    <material name="gray" rgba="0.4627 0.4627 0.4627 1"/>
    <material name="gripper_black" rgba="0.149 0.149 0.149 1"/>

    <mesh class="2f85" file="robotiq2/base_mount.stl"/>
    <mesh class="2f85" file="robotiq2/gripper_base.stl"/>
    <mesh class="2f85" file="robotiq2/driver.stl"/>
    <mesh class="2f85" file="robotiq2/coupler.stl"/>
    <mesh class="2f85" file="robotiq2/follower.stl"/>
    <mesh class="2f85" file="robotiq2/pad.stl"/>
    <mesh class="2f85" file="robotiq2/silicone_pad.stl"/>
    <mesh class="2f85" file="robotiq2/spring_link.stl"/>
  </asset>

  <actuator>
    <position class="large_actuator" name="act1" joint="J1" ctrlrange="-3.49066 3.49066"/>
    <position class="large_actuator" name="act2" joint="J2" ctrlrange="-3.13985 3.13985"/>
    <position class="large_actuator" name="act3" joint="J3" ctrlrange="-1.18682 4.32842"/>
    <position class="small_actuator" name="act4" joint="J4" ctrlrange="-3.31613 3.31613"/>
    <position class="small_actuator" name="act5" joint="J5" ctrlrange="-3.13985 3.13985"/>
    <position class="small_actuator" name="act6" joint="J6" ctrlrange="-3.92699 3.92699"/>
    <!-- gripper: robotiq_2f85 -->
    <general class="2f85" name="fingers_actuator" tendon="split" forcerange="-5 5" ctrlrange="0 255"
             gainprm="0.3137255 0 0" biasprm="0 -100 -10"/>
  </actuator>

  <sensor>
    <force name="force_sensor" site="force_sensor"/>
    <torque name="torque_sensor" site="force_sensor"/>
  </sensor>

  <!-- gripper: robotiq_2f85 -->
  <contact>
    <exclude body1="gripper_base" body2="left_driver"/>
    <exclude body1="gripper_base" body2="right_driver"/>
    <exclude body1="gripper_base" body2="left_spring_link"/>
    <exclude body1="gripper_base" body2="right_spring_link"/>
    <exclude body1="right_coupler" body2="right_follower"/>
    <exclude body1="left_coupler" body2="left_follower"/>
  </contact>

  <tendon>
    <fixed name="split">
      <joint joint="right_driver_joint" coef="0.5"/>
      <joint joint="left_driver_joint" coef="0.5"/>
    </fixed>
  </tendon>

  <equality>
    <connect body1="right_follower" body2="right_coupler" anchor="0 0 0"
             solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <connect body1="left_follower" body2="left_coupler" anchor="0 0 0"
             solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <joint joint1="right_driver_joint" joint2="left_driver_joint"
           polycoef="0 1 0 0 0" solimp="0.95 0.99 0.001" solref="0.005 1"/>
  </equality>

</mujoco>
